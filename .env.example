# MySQL Backend Credentials
# Username - defaults to 'root' if not set
MYSQL_USER=root

# Password
# - Set to your password: MYSQL_PASSWORD=mypassword
# - Leave blank for no password: MYSQL_PASSWORD=
# - Or omit entirely (will default to empty password)
MYSQL_PASSWORD=your_secure_password_here

# Optional: Override config file location
CONFIG_FILE=config/config.yaml

# Optional: Log level override
LOG_LEVEL=INFO
